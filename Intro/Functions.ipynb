        "id": "zQGnJZlp6f6f",
        "colab": {}
      },
      "source": [
        "def do_i_have_too_many_cats(num_cats):\n",
        "  if num_cats <= 15:\n",
        "    return \"no\"\n",
        "  else:\n",
        "    return \"yes\"\n",
        "  \n",
        "print(do_i_have_too_many_cats(3))\n",
        "print(do_i_have_too_many_cats(65))\n",
        "\n",
        "my_cats = 437\n",
        "\n",
        "print(do_i_have_too_many_cats(my_cats))"
      ],
