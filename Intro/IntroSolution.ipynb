   "source": [
    "from datetime import date\n",
    "def leap_year():\n",
    "    year = date.today().year\n",
    "    year = int(year)\n",
    "    if(year % 4 == 0):\n",
    "        print(\"It's a leap year!\")\n",
    "    else:\n",
    "        print(\"It's not a leap year\")\n",
    "\n",
    "leap_year()"
   ]
